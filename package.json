{
  "name": "my-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch token/designTokens.json --exec \"npm run build-tokens && next dev -H 0.0.0.0\"",
    "build": "npm run build-tokens && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.js\" \"**/*.jsx\" \"**/*.ts\" \"**/*.tsx\"",
    "api:build": "aspida",
    "build-tokens": "./node_modules/.bin/token-transformer token/designTokens.json token/tokens.json && style-dictionary build --config ./config.json"
  },
  "dependencies": {
    "@next/font": "^13.4.8",
    "@types/node": "20.3.0",
    "@types/react": "18.2.11",
    "@types/react-dom": "18.2.4",
    "aspida": "^1.12.0",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.5",
    "microcms-js-sdk": "^2.5.0",
    "next": "^13.5.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-svg": "^16.1.19",
    "swiper": "^9.4.1",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "@aspida/fetch": "^1.12.0",
    "@styled-jsx/plugin-sass": "^4.1.0",
    "@types/youtube": "^0.0.47",
    "nodemon": "^3.0.3",
    "prettier": "^2.8.8",
    "sass": "^1.70.0",
    "token-transformer": "^0.0.33"
  }
}
